{
  "name": "denton-sun---retro-os-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:bundle": "esbuild index.tsx --bundle --outfile=index.js --format=esm --jsx=automatic --platform=browser --external:react --external:react-dom --external:react/jsx-runtime --external:react-rnd --external:lucide-react --external:react-icons/fa --define:import.meta.env.PROD=false",
    "build:prod": "esbuild index.tsx --bundle --outfile=index.js --format=esm --jsx=automatic --platform=browser --external:react --external:react-dom --external:react/jsx-runtime --external:react-rnd --external:lucide-react --external:react-icons/fa --define:import.meta.env.PROD=true --define:import.meta.env.BASE_URL='\"\"'",
    "preview": "vite preview",
    "deploy:user": "npm run build:prod && gh-pages -d . -b main -r https://github.com/Dentsun/dentsun.github.io.git",
    "deploy:project": "npm run build:prod && gh-pages -d . -b gh-pages"
  },
  "dependencies": {
    "react": "^19.2.0",
    "lucide-react": "^0.554.0",
    "react-dom": "^19.2.0",
    "react-rnd": "^10.5.2",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^6.2.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
